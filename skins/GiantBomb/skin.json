{
  "name": "GiantBomb",
  "author": "Giant Bomb Wiki Street Team",
  "url": "https://www.giantbomb.com/",
  "description": "A custom MediaWiki skin for the Giant Bomb Wiki.",
  "license-name": "MIT",
  "type": "skin",
  "ValidSkinNames": {
    "giantbomb": {
      "class": "SkinGiantBomb",
      "args": [
        {
          "name": "giantbomb",
          "template": "GiantBombTemplate",
          "styles": ["skins.giantbomb"]
        }
      ]
    }
  },
  "AutoloadClasses": {
    "SkinGiantBomb": "includes/SkinGiantBomb.php",
    "GiantBombTemplate": "includes/GiantBombTemplate.php",
    "GiantBomb\\Skin\\Helpers\\PageHelper": "includes/helpers/PageHelper.php"
  },
  "ResourceModules": {
    "skins.giantbomb.vueexamplecomponent": {
      "scripts": ["resources/components/VueExampleComponent.js"]
    },
    "skins.giantbomb.styles": {
      "styles": [
        "resources/css/styles.css",
        "resources/css/pageWrapper.css",
        "resources/css/search.css",
        "resources/css/wikiNav.css",
        "resources/css/landingPage.css",
        "resources/css/gamePage.css",
        "resources/css/newReleasesPage.css",
        "resources/css/filters.css",
        "resources/css/itemGrid.css",
        "resources/css/viewToggle.css",
        "resources/css/platformsPage.css"
      ]
    },
    "skins.giantbomb.externalheader": {
      "scripts": [
        "resources/js/loadExternalHeader.js",
        "resources/js/search.js"
      ],
      "position": "top"
    },
    "skins.giantbomb": {
      "packageFiles": [
        "resources/components/index.js",
        "resources/components/VueSingleFileComponentExample.vue",
        "resources/components/AnotherVueComponent.vue",
        "resources/components/DatabaseAddPrompt.vue",
        "resources/components/GameImageViewer.vue",
        "resources/components/ReleaseSelector.vue",
        "resources/components/ViewToggle.vue",
        "resources/components/ReleaseFilter.vue",
        "resources/components/ReleaseList.vue",
        "resources/components/GameFilter.vue",
        "resources/components/GameList.vue",
        "resources/components/Pagination.vue",
        "resources/components/PlatformFilter.vue",
        "resources/components/PlatformList.vue",
        "resources/components/FilterContainer.vue",
        "resources/components/FilterDropdown.vue",
        "resources/components/FilterInput.vue",
        "resources/components/SearchableMultiSelect.vue",
        "resources/composables/index.js",
        "resources/composables/useFilters.js",
        "resources/composables/useSearch.js",
        "resources/helpers/countryFlags.js",
        "resources/helpers/htmlUtils.js",
        "resources/data/countryFlags.json"
      ],
      "dependencies": ["skins.giantbomb.vueexamplecomponent", "vue"]
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "",
    "remoteSkinPath": "GiantBomb"
  },
  "manifest_version": 2
}
