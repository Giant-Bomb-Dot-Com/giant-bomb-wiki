{
  "engines": {
    "node": ">=24",
    "pnpm": ">=10.15"
  },
  "devDependencies": {
    "cypress": "^14.5.2",
    "cypress-dotenv": "^3.0.1",
    "dotenv": "^17.2.2",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "format": "pnpm prettier --write --config .prettierrc 'README.md|(skins|cypress|.github)/**/*.{js,json,ts,yaml}|*.json|*.yaml'",
    "format:check": "pnpm prettier --check --config .prettierrc 'README.md|(skins|cypress|.github)/**/*.{js,json,ts,yaml}|*.json|*.yaml'",
    "test:e2e": "pnpm cypress run"
  }
}
